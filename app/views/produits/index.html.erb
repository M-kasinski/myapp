<p id="notice"><%= notice %></p>

<h1>Listing Produits</h1>

<table class='table table-bordered'>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Marque</th>
      <th>Prix ha</th>
      <th>Remise</th>
      <th>Coef</th>
      <th>Prix</th>
      <th>Justif remise</th>
      <th>Date remise</th>
      <th>Categorie</th>
      <th>Saison</th>
      <th>Annee s</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @produits.each do |produit| %>
      <tr>
        <td><%= produit.nom %></td>
        <td><%= produit.marque %></td>
        <td><%= produit.prix_ha %></td>
        <td><%= produit.remise %></td>
        <td><%= produit.coef %></td>
        <td><%= produit.prix %></td>
        <td><%= produit.justif_remise %></td>
        <td><%= produit.date_remise %></td>
        <td><%= produit.categorie %></td>
        <td><%= produit.saison %></td>
        <td><%= produit.annee_s %></td>
        <td><%= link_to 'Show', produit %></td>
        <td><%= link_to 'Edit', edit_produit_path(produit) %></td>
        <td><%= link_to 'Destroy', produit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Produit', new_produit_path %>
