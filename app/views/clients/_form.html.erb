<%= simple_form_for(@client) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nom %>
    <%= f.input :prenom %>
    <%= f.input :tel %>
    <%= f.input :ville %>
    <%= f.input :cp %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
